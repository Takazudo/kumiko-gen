{
  "name": "kumiko-gen",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "// ── Packages ────────────────────────────────────": "",
    "build": "pnpm --filter @takazudo/kumiko-gen build",
    "test": "pnpm --filter @takazudo/kumiko-gen test",
    "// ── Viewer ──────────────────────────────────────": "",
    "viewer:dev": "pnpm --filter kumiko-gen-viewer dev",
    "viewer:build": "pnpm --filter kumiko-gen-viewer build",
    "// ── Doc site ────────────────────────────────────": "",
    "doc:start": "cd doc && pnpm run start",
    "doc:build": "cd doc && pnpm run build",
    "doc:serve": "cd doc && pnpm run serve",
    "doc:generate": "cd doc && pnpm run generate",
    "doc:typecheck": "cd doc && pnpm run typecheck",
    "// ── Validation ──────────────────────────────────": "",
    "b4push": "./scripts/run-b4push.sh",
    "// ── Internal & utilities ────────────────────────": "",
    "_doc:install": "cd doc && pnpm install"
  },
  "pnpm": {
    "overrides": {
      "chokidar": "^5.0.0",
      "undici-types": "^7.22.0"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
  }
}
