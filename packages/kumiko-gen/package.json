{
  "name": "@takazudo/kumiko-gen",
  "version": "0.1.0",
  "description": "Generate deterministic Japanese kumiko geometric pattern SVGs from text seeds",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./svg-to-png": {
      "types": "./dist/svg-to-png/index.d.ts",
      "default": "./dist/svg-to-png/index.js"
    }
  },
  "bin": {
    "kumiko-gen": "./dist/cli.js",
    "kumiko-gen-svg-to-png": "./dist/svg-to-png/cli.js"
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "prepublishOnly": "tsup && vitest run"
  },
  "keywords": ["kumiko", "japanese", "geometric", "pattern", "svg", "generative", "cli"],
  "author": "Takazudo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Takazudo/kumiko-gen.git"
  },
  "bugs": {
    "url": "https://github.com/Takazudo/kumiko-gen/issues"
  },
  "homepage": "https://kumiko-gen.netlify.app/pj/kumiko-gen/doc/",
  "engines": {
    "node": ">=18.0.0"
  },
  "optionalDependencies": {
    "sharp": "^0.33.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
